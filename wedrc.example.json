{
  "tasks": [
    "indent",
    "shell"
  ],
  "commands": [
    "echo",
    "file"
  ],
  "wrappers": {
    "exit": "shell.toggle"
  },
  "keymap": {
    "editor": {
      "Ctrl-C": "shell.toggle",
      "Tab": "indent.useSpaces"
    },
    "shell": {
      "Ctrl-C": "shell.toggle"
    }
  },
  "editor": {
    "indentUnit": 4,
    "lineNumbers": true
  }
}
